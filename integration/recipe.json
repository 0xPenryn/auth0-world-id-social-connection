{
  "authorizationURL": "https://id.worldcoin.org/authorize",
  "tokenURL": "https://id.worldcoin.org/token",
  "permissions": [
    {
      "scope": "openid",
      "label": "OpenID",
      "required": true,
      "default": true
    }
  ],
  "configuration": [
    {
      "name": "client_id",
      "label": "Client ID",
      "description": "Public unique identifier for this application.",
      "required": true
    },
    {
      "name": "client_secret",
      "label": "Client Secret",
      "description": "Secret for this application.",
      "required": true
    }
  ]
}
